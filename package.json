{
  "name": "rodate",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=22.14.0 <23.0.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "gulp bundle",
    "clean": "gulp clean",
    "test": "gulp test",
    "bundle": "npm-run-all tailwindscss build",
    "bundle:ship": "npm-run-all tailwindcss build:ship",
    "webpack:serve": "fast-serve",
    "serve": "npm-run-all -p tailwindcss:watch webpack:serve",
    "tailwindcss": "tailwindcss -i ./assets/tailwind.css -o ./assets/dist/tailwind.css --minify --postcss ./postcss.config.js",
    "tailwindcss:watch": "tailwindcss -i ./assets/tailwind.css -o ./assets/dist/tailwind.css --watch --minify --postcss ./postcss.config.js",
    "publish": "gulp clean && gulp build && gulp bundle --ship",
    "postpublish": "gulp package-solution --ship"
  },
  "dependencies": {
    "@fluentui/react": "^8.106.4",
    "@fluentui/react-components": "^9.49.1",
    "@microsoft/rush-stack-compiler-5.3": "^0.1.0",
    "@microsoft/sp-adaptive-card-extension-base": "1.21.1",
    "@microsoft/sp-component-base": "1.21.1",
    "@microsoft/sp-core-library": "1.21.1",
    "@microsoft/sp-lodash-subset": "1.21.1",
    "@microsoft/sp-office-ui-fabric-core": "1.21.1",
    "@microsoft/sp-property-pane": "1.21.1",
    "@microsoft/sp-webpart-base": "1.21.1",
    "@pnp/graph": "^4.11.0",
    "@pnp/sp": "^4.11.0",
    "dotenv": "^16.4.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@microsoft/eslint-config-spfx": "1.21.1",
    "@microsoft/eslint-plugin-spfx": "1.21.1",
    "@microsoft/rush-stack-compiler-4.7": "0.1.0",
    "@microsoft/sp-build-web": "1.21.1",
    "@microsoft/sp-http-base": "1.21.0",
    "@microsoft/sp-module-interfaces": "1.21.1",
    "@rushstack/eslint-config": "4.0.1",
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.17",
    "@types/webpack-env": "~1.15.2",
    "autoprefixer": "^9.8.6",
    "eslint": "8.57.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "gulp": "^4.0.2",
    "gulp-postcss": "^10.0.0",
    "gulp-util": "^3.0.8",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "spfx-fast-serve-helpers": "~1.21.0",
    "tailwindcss": "3.4.13",
    "typescript": "5.3.3"
  }
